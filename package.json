{
  "name": "logseq-plugin-task-check-date",
  "version": "0.0.10",
  "description": "Plugin enables adding 'completed' property to task upon checking",
  "targets": {
    "main": false
  },
  "default": "dist/index.html",
  "author": "Dmitrii Dushkin (https://github.com/DimitryDushkin)",
  "license": "MIT",
  "scripts": {
    "dev": "parcel ./index.html --public-url ./",
    "build": "rm -rf dist/* && parcel build --public-url . --no-source-maps index.html"
  },
  "devDependencies": {
    "parcel": "^2.12.0",
    "typescript": "^5.4.5"
  },
  "logseq": {
    "main": "dist/index.html",
    "title": "Task completion",
    "id": "_sppzszglx",
    "icon": "icon.png"
  },
  "packageManager": "yarn@3.2.4",
  "dependencies": {
    "@logseq/libs": "^0.0.17",
    "dayjs": "^1.11.11",
    "logseq-dateutils": "^2.1.1"
  }
}
