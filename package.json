  },
  "main": "dist/index.js",
  "source": "index.ts",
  "targets": {
    "main": {
       "includeNodeModules": true
    }
 },
  "packageManager": "yarn@3.2.4",
  "dependencies": {
    "@logseq/libs": "^0.0.10",
    "logseq-dateutils": "^0.0.22"
  }
